{
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["ES2022", "DOM", "DOM.Iterable"],
    "module": "ESNext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "allowJs": true,
    "checkJs": false,
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "outDir": "./dist",
    "removeComments": true,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "strictBindCallApply": true,
    "strictPropertyInitialization": true,
    "noImplicitThis": true,
    "useUnknownInCatchVariables": true,
    "alwaysStrict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "exactOptionalPropertyTypes": false,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "noUncheckedIndexedAccess": true,
    "noImplicitOverride": true,
    "noPropertyAccessFromIndexSignature": false,
    "allowUnusedLabels": false,
    "allowUnreachableCode": false,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "skipLibCheck": true,
    "incremental": true,
    "composite": true,
    "baseUrl": ".",
    "paths": {
      "@ai-core/types": ["libs/shared/types/src/index.ts"],
      "@ai-core/types/*": ["libs/shared/types/src/*"],
      "@ai-core/utils": ["libs/shared/utils/src/index.ts"],
      "@ai-core/utils/*": ["libs/shared/utils/src/*"],
      "@ai-core/constants": ["libs/shared/constants/src/index.ts"],
      "@ai-core/constants/*": ["libs/shared/constants/src/*"],
      "@ai-core/ui": ["libs/ui/src/index.ts"],
      "@ai-core/ui/*": ["libs/ui/src/*"],
      "@ai-core/database": ["libs/database/src/index.ts"],
      "@ai-core/database/*": ["libs/database/src/*"],
      "@ai-core/ai": ["libs/ai-core/src/index.ts"],
      "@ai-core/ai/*": ["libs/ai-core/src/*"],
      "@ai-core/integrations": ["libs/integrations/src/index.ts"],
      "@ai-core/integrations/*": ["libs/integrations/src/*"],
      "@ai-core/ai-agents": ["libs/ai-agents/src/index.ts"],
      "@ai-core/ai-agents/*": ["libs/ai-agents/src/*"],
      "@ai-core/ai-analytics": ["libs/ai-analytics/src/index.ts"],
      "@ai-core/ai-analytics/*": ["libs/ai-analytics/src/*"],
      "@ai-core/ai-comms": ["libs/ai-comms/src/index.ts"],
      "@ai-core/ai-comms/*": ["libs/ai-comms/src/*"],
      "@ai-core/ai-communications": ["libs/ai-communications/src/index.ts"],
      "@ai-core/ai-communications/*": ["libs/ai-communications/src/*"],
      "@ai-core/ai-documents": ["libs/ai-documents/src/index.ts"],
      "@ai-core/ai-documents/*": ["libs/ai-documents/src/*"],
      "@ai-core/ai-finance": ["libs/ai-finance/src/index.ts"],
      "@ai-core/ai-finance/*": ["libs/ai-finance/src/*"],
      "@ai-core/ai-gateway": ["libs/ai-gateway/src/index.ts"],
      "@ai-core/ai-gateway/*": ["libs/ai-gateway/src/*"],
      "@ai-core/ai-hr": ["libs/ai-hr/src/index.ts"],
      "@ai-core/ai-hr/*": ["libs/ai-hr/src/*"],
      "@ai-core/ai-iam": ["libs/ai-iam/src/index.ts"],
      "@ai-core/ai-iam/*": ["libs/ai-iam/src/*"],
      "@ai-core/ai-insurance": ["libs/ai-insurance/src/index.ts"],
      "@ai-core/ai-insurance/*": ["libs/ai-insurance/src/*"],
      "@ai-core/ai-integrations": ["libs/ai-integrations/src/index.ts"],
      "@ai-core/ai-integrations/*": ["libs/ai-integrations/src/*"],
      "@ai-core/ai-leads": ["libs/ai-leads/src/index.ts"],
      "@ai-core/ai-leads/*": ["libs/ai-leads/src/*"],
      "@ai-core/ai-llm": ["libs/ai-llm/src/index.ts"],
      "@ai-core/ai-llm/*": ["libs/ai-llm/src/*"],
      "@ai-core/ai-mdm": ["libs/ai-mdm/src/index.ts"],
      "@ai-core/ai-mdm/*": ["libs/ai-mdm/src/*"],
      "@ai-core/ai-portal": ["libs/ai-portal/src/index.ts"],
      "@ai-core/ai-portal/*": ["libs/ai-portal/src/*"],
      "@ai-core/ai-projects": ["libs/ai-projects/src/index.ts"],
      "@ai-core/ai-projects/*": ["libs/ai-projects/src/*"],
      "@ai-core/ai-voice": ["libs/ai-voice/src/index.ts"],
      "@ai-core/ai-voice/*": ["libs/ai-voice/src/*"],
      "@ai-core/ai-workflows": ["libs/ai-workflows/src/index.ts"],
      "@ai-core/ai-workflows/*": ["libs/ai-workflows/src/*"]
    }
  },
  "exclude": ["node_modules", "dist", "build", ".turbo", "coverage"]
}
