{
  "name": "@ait-core/nerve",
  "version": "1.0.0",
  "description": "Network Engine Runtime & Vital Executor - El Sistema Nervioso Central del Ecosistema AIT-CORE",
  "keywords": [
    "ait-core",
    "motor-management",
    "orchestration",
    "monitoring",
    "microservices",
    "control-plane"
  ],
  "homepage": "https://docs.ait-core.com/ait-nerve",
  "repository": {
    "type": "git",
    "url": "https://github.com/soriano-mediadores/ait-core.git",
    "directory": "packages/ait-nerve"
  },
  "license": "PROPRIETARY",
  "author": {
    "name": "AIT-CORE Team",
    "email": "dev@ait-core.com",
    "url": "https://ait-core.com"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "prisma",
    "README.md"
  ],
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsup",
    "start": "node dist/index.js",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "pnpm lint --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,md}\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:prod": "prisma migrate deploy",
    "prisma:seed": "tsx prisma/seed.ts",
    "prisma:studio": "prisma studio",
    "prisma:reset": "prisma migrate reset --force",
    "db:setup": "pnpm prisma:migrate && pnpm prisma:seed",
    "db:push": "prisma db push",
    "clean": "rimraf dist",
    "prebuild": "pnpm clean && pnpm prisma:generate",
    "postinstall": "pnpm prisma:generate",
    "docker:build": "docker build -t ait-nerve:latest .",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
  "dependencies": {
    "@prisma/client": "^5.20.0",
    "@trpc/server": "^10.45.0",
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.4.0",
    "cors": "^2.8.5",
    "date-fns": "^3.3.1",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "prom-client": "^15.1.0",
    "socket.io": "^4.6.1",
    "superjson": "^2.2.1",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.5",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vitest/coverage-v8": "^1.2.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "prettier": "^3.2.4",
    "prisma": "^5.20.0",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.1"
}
