{
  "module": {
    "id": "ait-underwriting",
    "name": "Underwriting Engine",
    "version": "1.0.0",
    "category": "insurance-specialized",
    "priority": "P0",
    "status": "active"
  },
  "connector": {
    "type": "nestjs-module",
    "autoLoad": true,
    "provides": ["UnderwritingService", "RiskAssessmentService"],
    "api": {
      "prefix": "/api/v1/underwriting",
      "endpoints": [
        "POST /assess",
        "POST /approve",
        "POST /decline",
        "GET /rules"
      ]
    }
  },
  "capabilities": {
    "domain": "Underwriting y Suscripción",
    "features": [
      "Evaluación automática de riesgos",
      "Reglas de underwriting configurables",
      "Scoring de riesgo 0-100",
      "Integración con insurance-specialist AI",
      "Integración con ait-engines (actuarial)",
      "Decisión automática: aprobar/declinar/referir",
      "Historial de decisiones",
      "Reporting de siniestralidad"
    ]
  }
}
