{
  "name": "@ait-core/document-service",
  "version": "1.0.0",
  "description": "Document management service with storage, generation, OCR, and search capabilities",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down"
  },
  "keywords": ["documents", "storage", "pdf", "ocr", "search"],
  "author": "AIT-CORE Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "multer": "^1.4.5-lts.1",
    "dotenv": "^16.3.1",
    "winston": "^3.11.0",
    "prom-client": "^15.1.0",
    "aws-sdk": "^2.1524.0",
    "@aws-sdk/client-s3": "^3.478.0",
    "@aws-sdk/s3-request-presigner": "^3.478.0",
    "minio": "^7.1.3",
    "pdfkit": "^0.14.0",
    "puppeteer": "^21.7.0",
    "officegen": "^0.6.5",
    "exceljs": "^4.4.0",
    "sharp": "^0.33.1",
    "tesseract.js": "^5.0.4",
    "pdf-parse": "^1.1.1",
    "mammoth": "^1.6.0",
    "@elastic/elasticsearch": "^8.11.0",
    "prisma": "^5.8.1",
    "@prisma/client": "^5.8.1",
    "uuid": "^9.0.1",
    "mime-types": "^2.1.35",
    "file-type": "^18.7.0",
    "crypto-js": "^4.2.0",
    "archiver": "^6.0.1",
    "unzipper": "^0.10.14"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "@types/multer": "^1.4.11",
    "@types/pdfkit": "^0.13.4",
    "@types/mime-types": "^2.1.4",
    "@types/crypto-js": "^4.2.1",
    "@types/archiver": "^6.0.2",
    "typescript": "^5.3.3",
    "tsx": "^4.7.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
